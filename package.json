  "main": "dist/main.js",
  "devDependencies": {
    "@types/progressbar.js": "^1.1.2",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "copyfiles": "^2.4.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "identicon.js": "^2.3.1",
    "jest": "21.2.1",
    "lorem-ipsum": "^1.0.4",
    "nwb": "^0.23.0",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "^1.0.10",
    "react-test-renderer": "^17.0.2",
    "ts-loader": "8.2.0",
    "typescript": "^4.6.2",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/detaysoft/react-chat-elements.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "chat",
    "css",
    "chat",
    "components",
    "detaysoft",
    "native",
    "react-native"
  ],
  "license": "MIT",
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "bugs": {
    "url": "https://github.com/detaysoft/react-chat-elements/issues"
  },
  "homepage": "https://github.com/detaysoft/react-chat-elements#readme",
  "scripts": {
    "start": "nwb serve-react-app ./example/index.tsx",
    "copy-files": "copyfiles -u 1 src/**/*.css build/",
    "start:native": "node node_modules/react-native/local-cli/cli.js start",
    "build": "node node_modules/typescript/bin/tsc && yarn copy-files && webpack --progress --profile --colors",
    "test": "react-scripts test --env=jsdom",
    "clean": "nwb clean-module && nwb clean-demo",
    "nwbstart": "nwb serve-react-demo",
    "test:coverage": "npm run test -- --coverage --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/index.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "progressbar.js": "^1.1.0",
    "react-icons": "^4.3.1",
    "timeago.js": "^4.0.0-beta.2"
  }
}
